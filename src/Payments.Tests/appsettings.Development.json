{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionsString": {
    "MongoDB": "mongodb://localhost:27017",
    "RabbitMq": "amqp://admin:admin123*@localhost:5672"
  },
  "MongoDB": {
    "DatabaseName": "Transactions"
  },
  "RabbitMQSettings": {
    "QueueName": "transactions",
    "MaxDeliveryAttempts": 2,
    "ErrorQueueName": "errors",
    "MaxParallelism": 10,
    "NumberOfWorkers": 1
  }
}
